print('Hello')
print('Lets play a guessing game')
import random
r_n=random.randint(1,10)
c=r_n
guess=None
while True:
	guess=int(input('enter a number from 1 to 10:\n'))
	if guess<c:
		print('sorry,too low')
	elif guess>c:
		print('sorry,too high')
	else:
		print('yayy!!you won!!')
		play_again=input('do you want to play again? (yes/no)\n')
		if play_again=='yes':
			r_n=random.randint(1,10)
			c=r_n
			guess=None
		else:
			print('Thank you')
			break
print('the number is:',c)
